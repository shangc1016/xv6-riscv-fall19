[12;1H       [1mint[0m [1mopen(const[0m [1mchar[0m [1m*[0m[4mpathname[24m[1m,[0m [1mint[0m [4m[7mflags[27m[24m[1m,[0m [1mmode_t[0m [4mmode[24m[1m);[0m[m
[13;1H[m
[14;1H       [1mint[0m [1mcreat(const[0m [1mchar[0m [1m*[0m[4mpathname[24m[1m,[0m [1mmode_t[0m [4mmode[24m[1m);[0m[m
